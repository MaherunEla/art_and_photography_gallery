generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Upload {
    id          String @id @default(uuid())
    title       String
    price       String
    artist      String
    description String
    image       String
}

model Signup {
    id         String @id @default(uuid())
    name       String
    contact    String
    email      String
    occupation String
    role       Role   @default(User)
}

enum Role {
    User
    artist
}
